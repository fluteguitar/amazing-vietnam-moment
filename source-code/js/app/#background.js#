let genRandomImage = function() {
    let index = getRandomInt(0, listImage.length -1 );
    return imagePath + listImage[index] + '.jpg';
} 
let app2 = new Vue({
    el: '.background',
    data: {
	styleObject: {
	    "background-color": '#ff6600',
	    "background-image": 'url(' + genRandomImage() + ')'
	},
	imgSrc: '2.jpg'
    }
})


